SERVER_PORT=3000
MAX_WORKSPACES_PER_USER=15
DOCKER_NETWORK=xares-aicoder-network

# Production configuration for sensem.de deployment
BASE_DOMAIN=coder.sensem.de
BASE_PORT=443
PROTOCOL=https

# Host port configuration for production
# This is the port that XaresAICoder will bind to on the sensem.de server
# External nginx will proxy from 443 (SSL) to this port
HOST_PORT=8080

# Optional: Adjust if port 8080 is already in use on sensem.de
# Alternative ports: 8081, 9000, 9001, etc.