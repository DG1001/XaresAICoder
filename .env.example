SERVER_PORT=3000
WORKSPACE_TIMEOUT_MINUTES=120
MAX_WORKSPACES_PER_USER=5
DOCKER_NETWORK=xares-aicoder-network

# Domain and port configuration
# For localhost development: BASE_DOMAIN=localhost, BASE_PORT=80
# For ci.infra deployment: BASE_DOMAIN=ci.infra, BASE_PORT=8000
# For production with SSL: BASE_DOMAIN=xares.example.com, BASE_PORT=443, PROTOCOL=https
BASE_DOMAIN=localhost
BASE_PORT=80
PROTOCOL=http

# Git Server Configuration (optional)
# Set to true to enable integrated Forgejo Git server
ENABLE_GIT_SERVER=false

# Git Server Admin Configuration (used only if ENABLE_GIT_SERVER=true)
GIT_ADMIN_USER=developer
GIT_ADMIN_PASSWORD=admin123!
GIT_ADMIN_EMAIL=admin@xaresaicoder.local
GIT_SITE_NAME=XaresAICoder Git Server
GIT_SITE_DESCRIPTION=Integrated Git server for XaresAICoder development environment